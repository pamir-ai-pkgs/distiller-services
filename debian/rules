#!/usr/bin/make -f

# Uncomment this to turn on verbose mode.
#export DH_VERBOSE=1

%:
	dh $@

override_dh_installsystemd:
	dh_installsystemd --name=distiller-wifi

override_dh_fixperms:
	dh_fixperms
	# Make main script executable
	chmod +x debian/distiller-services/opt/distiller-services/distiller_wifi.py
	# Make all shell scripts executable
	-find debian/distiller-services/opt/distiller-services/scripts -name "*.sh" -exec chmod +x {} \;
