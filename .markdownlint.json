{
  "default": true,
  "MD003": { "style": "atx" },
  "MD004": { "style": "dash" },
  "MD007": { "indent": 2 },
  "MD013": {
    "line_length": 100,
    "heading_line_length": 100,
    "code_block_line_length": 100,
    "code_blocks": true,
    "tables": false,
    "headings": true,
    "headers": true,
    "strict": false,
    "stern": false
  },
  "MD024": { "siblings_only": true },
  "MD025": { "level": 1, "front_matter_title": "" },
  "MD026": { "punctuation": ".,;:!" },
  "MD029": { "style": "ordered" },
  "MD030": {
    "ul_single": 1,
    "ol_single": 1,
    "ul_multi": 1,
    "ol_multi": 1
  },
  "MD033": { "allowed_elements": ["details", "summary", "br", "img", "a"] },
  "MD035": { "style": "---" },
  "MD036": { "punctuation": ".,;:!?。，；：！？" },
  "MD041": { "level": 1, "front_matter_title": "" },
  "MD044": {
    "names": [
      "JavaScript",
      "HTML",
      "CSS",
      "WiFi",
      "API",
      "JSON",
      "YAML",
      "Python",
      "FastAPI",
      "WebSocket",
      "mDNS",
      "Raspberry Pi",
      "CM5",
      "Distiller"
    ],
    "code_blocks": false
  },
  "MD046": { "style": "fenced" },
  "MD048": { "style": "backtick" },
  "MD049": { "style": "underscore" },
  "MD050": { "style": "asterisk" }
}
